	function log(address p0, address p1, address p2, string memory p3) internal view {
		_sendLogPayload(abi.encodeWithSignature("log(address,address,address,string)", p0, p1, p2, p3));
	}

	function log(address p0, address p1, address p2, bool p3) internal view {
		_sendLogPayload(abi.encodeWithSignature("log(address,address,address,bool)", p0, p1, p2, p3));
	}

	function log(address p0, address p1, address p2, address p3) internal view {
		_sendLogPayload(abi.encodeWithSignature("log(address,address,address,address)", p0, p1, p2, p3));
	}

}


interface IMinter {

    function notifyMint(uint256 cusdAmount, uint256 collateralAmount) external;
    
    function getMintWithPercent(uint256 percent) external;
    
    function reinvestMint() external;
    
}


interface IRedeemer {

    function notifyRedeem(uint256 cusdAmount, uint256 collateral0Amount, uint256 collateral1Amount) external;

    function getRedeemWithPercent(uint256 percent) external;

    function reinvestRedeem() external;
    
}


contract CapcMinterUsdc is CapcMinter {

    IERC20 public USDC;
    address public usdcAddress;

    
    constructor(address usdc, address cusd, address cpt) {

        USDC = IERC20(usdc);
        usdcAddress = usdc;

        coinCUSD = CUSD(cusd);
        cusdAddress = cusd;

        coinCPT = CPT(cpt);
        cptAddress = cpt;
    }

    function determine() external onlyGovernor override {

        uint256 cusdPrice = capcPricer.getCUSDPrice();
        uint256 cptPrice = capcPricer.getCPTPrice();
        uint256 usdcPrice = capcPricer.getUSDCPrice();
        uint256 colRatio = capcPricer.collateralRatio();

        uint256 cusdValue = randomBetween(100e18, 500e18);

        if (cusdPrice > CAPC_DECIMAL + capcPricer.priceBand()) {
            mint(cusdValue * colRatio / usdcPrice, cusdValue * (CAPC_DECIMAL - colRatio) / cptPrice, cusdValue);
            lastMintRatio = cusdPrice - CAPC_DECIMAL;
        } else if (cusdPrice < CAPC_DECIMAL - capcPricer.priceBand()) {
            redeem(cusdValue, cusdValue * colRatio / usdcPrice, cusdValue * (CAPC_DECIMAL - colRatio) / cptPrice);
            lastRedeemRatio = CAPC_DECIMAL - cusdPrice;
        }

        capcPricer.updateCollateralRatio();
    }

    function randomBetween(uint256 min, uint256 max) internal view returns (uint256){
        require(min < max, "require min < max");
        uint256 distance = max - min;
        uint256 randomPercent = uint256(keccak256(abi.encodePacked(block.difficulty, block.timestamp, msg.sender, distance))) % 100;
        return min + distance * randomPercent / 100;
    }


    function mint(uint256 usdcAmount, uint256 cptAmount, uint256 cusdOut) internal override {

        uint256 colRaito = capcPricer.collateralRatio();
        uint256 cusdToUsdc = cusdOut * colRaito / CAPC_DECIMAL;
        uint256 cusdToCrs = cusdOut * (1e6 - colRaito) / CAPC_DECIMAL;

        address usdcStakingAddr = stakingAddress[usdcAddress];
        address cptStakingAddr = stakingAddress[cptAddress];

        IMinter(usdcStakingAddr).notifyMint(cusdToUsdc, usdcAmount);
        IMinter(cptStakingAddr).notifyMint(cusdToCrs, cptAmount);

        coinCUSD.poolMint(usdcStakingAddr, cusdToUsdc);
        coinCUSD.poolMint(cptStakingAddr, cusdToCrs);

    }

    function redeem(uint256 cusdAmount, uint256 usdcOut, uint256 cptOut) internal override {

        address cusdStakingAddr = stakingAddress[cusdAddress];

        IRedeemer(cusdStakingAddr).notifyRedeem(cusdAmount, cptOut, usdcOut);
        USDC.transfer(cusdStakingAddr, usdcOut);
        coinCPT.poolMint(cusdStakingAddr, cptOut);

    }


    function migrate(address newPool) external onlyOwner {
        uint256 amount = USDC.balanceOf(address(this));
        USDC.transfer(newPool, amount);
    }
}
